<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="vertical">

    <TextView
        android:id="@+id/textViewNome"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginStart="@dimen/margem_campos"
        android:layout_marginLeft="@dimen/margem_campos"
        android:layout_marginTop="@dimen/margem_campos"
        android:layout_marginEnd="@dimen/margem_campos"
        android:layout_marginRight="@dimen/margem_campos"
        android:textAppearance="@style/TextAppearance.AppCompat.Large"
        tools:text="Fábio Emanuel Fiqueli Abreu" />

    <TextView
        android:id="@+id/textViewNumero_de_Utente"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_margin="@dimen/margem_campos"
        android:textAppearance="@style/TextAppearance.AppCompat.Small"
        tools:text="763975519" />

    <TextView
        android:id="@+id/textViewSexo"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_margin="@dimen/margem_campos"
        android:textAppearance="@style/TextAppearance.AppCompat.Small"
        tools:text="Masculino" />

    <TextView
        android:id="@+id/textViewData_Nascimento"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_margin="@dimen/margem_campos"
        android:textAppearance="@style/TextAppearance.AppCompat.Small"
        tools:text="21-07-1999" />

    <TextView
        android:id="@+id/textViewTelemovel"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_margin="@dimen/margem_campos"
        android:textAppearance="@style/TextAppearance.AppCompat.Small"
        tools:text="931770700" />

    <TextView
        android:id="@+id/textViewEmail"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_margin="@dimen/margem_campos"
        android:textAppearance="@style/TextAppearance.AppCompat.Small"
        tools:text="fabiofiqueli@hotmail.com" />

    <TextView
        android:id="@+id/textViewMorada"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_margin="@dimen/margem_campos"
        android:textAppearance="@style/TextAppearance.AppCompat.Small"
        tools:text="Estrada Regional 104, Ribeira Brava, nº11" />
</LinearLayout>